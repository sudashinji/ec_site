<div class="container">
	<h2>会員一覧画面</h2>
	<table>
		<thead>
			<tr>
				<th scope="col">会員ID</th>
				<th scope="col">氏名</th>
				<th scope="col">メールアドレス</th>
				<th scope="col">ステータス</th>
			</tr>
		</thead>
		<tbody>
			<% @end_users.each do |end_user| %>
				<tr>
					<td><%= end_user.id %></td>
		            <td><%= link_to "#{end_user.last_name } #{end_user.first_name}", edit_admins_end_user_path(end_user.id) %></td>
		            <td><%= end_user.email %></td>

		            <% if end_user.deleted_at != nil %>
		             <td>退会済</td>
		            <% else %>
		             <td>有効</td>
		            <% end %>
				</tr>
		    <% end %>
		</tbody>
	</table>
</div>

